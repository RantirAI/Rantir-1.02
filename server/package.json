{
	"name": "rantir-backend",
	"version": "1.02",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "npx rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prisma:generate": "prisma generate",
		"prisma:migrate-dev": "prisma migrate dev",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e": "dotenv -e .env.test -- prisma migrate reset --force",
		"test:e2e": "dotenv -e .env.test -- jest -i --no-cache --config ./test/jest-e2e.json --verbose --forceExit",
		"test:cov-e2e": "dotenv -e .env.test -- jest --coverage --config ./test/jest-e2e.json",
		"prisma:dev:migrate": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy"
	},
	"resolutions": {
		"strip-ansi": "6.0.0"
	},
	"dependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^8.0.1",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^8.2.1",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/schedule": "^3.0.1",
		"@nestjs/swagger": "^6.2.1",
		"argon2": "^0.28.5",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"csv-writer": "^1.6.0",
		"jsonwebtoken": "^9.0.0",
		"jwt-decode": "^3.1.2",
		"nestjs-cls": "^3.3.1",
		"papaparse": "^5.4.1",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"pdfmake": "^0.2.5",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "27.4.1",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.0.0",
		"@types/papaparse": "^5.3.7",
		"@types/passport-jwt": "^3.0.6",
		"@types/pdfmake": "^0.1.21",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"dotenv-cli": "^6.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.2.5",
		"prettier": "^2.3.2",
		"prisma": "^4.11.0",
		"source-map-support": "^0.5.20",
		"supertest": "^6.3.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5"
	},
	"release": {
		"branches": [
			"main"
		]
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "./",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage/unit",
		"testEnvironment": "node"
	}
}
